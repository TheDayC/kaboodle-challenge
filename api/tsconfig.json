{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "node",
        "target": "ESNext",
        "outDir": "./dist",
        "baseUrl": ".",
        "strict": true,
        "allowJs": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}
